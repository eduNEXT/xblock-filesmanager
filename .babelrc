{
    "presets": ["@babel/preset-env", ["@babel/preset-react", { "runtime": "automatic" }]],
    "plugins": [
        [
          "module-resolver",
          {
            "root": ["./react-app"],
            "alias": {
              "@components": "./react-app/components",
              "@constants": "./react-app/constants",
              "@config": "./react-app/config",
              "@services": "./react-app/services",
              "@hooks": "./react-app/hooks"
            }
          }
        ]
      ]
}
